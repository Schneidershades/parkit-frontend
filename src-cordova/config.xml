<?xml version='1.0' encoding='utf-8'?>
<widget id="parkitLM.cordova.quasar.app" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Express by Parkit</name>
    <description>Parkit is a technology driven vehicle wash and care service. We pride ourselves in having; unparalleled customer experience; with our easy to use booking, payment and location finder system via our app, customers enjoy seamless booking experiences.</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="SplashScreen" value="screen" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <preference name="loadUrlTimeoutValue" value="700000" />
        <allow-intent href="market:*" />
        <icon qualifier="ldpi" src="resources/icons/ldpi.png" />
        <icon background="resources/android/icon/ldpi.png" density="ldpi" foreground="resources/android/icon/ldpi.png" src="resources/android/icon/ldpi.png" />
        <icon background="resources/android/icon/mdpi.png" density="mdpi" foreground="resources/android/icon/mdpi.png" src="resources/android/icon/mdpi.png" />
        <icon background="resources/android/icon/hdpi.png" density="hdpi" foreground="resources/android/icon/hdpi.png" src="resources/android/icon/hdpi.png" />
        <icon background="resources/android/icon/xhdpi.png" density="xhdpi" foreground="resources/android/icon/xhdpi.png" src="resources/android/icon/xhdpi.png" />
        <icon background="resources/android/icon/xxhdpi.png" density="xxhdpi" foreground="resources/android/icon/xxhdpi.png" src="resources/android/icon/xxhdpi.png" />
        <icon background="resources/android/icon/xxxhdpi.png" density="xxxhdpi" foreground="resources/android/icon/xxxhdpi.png" src="resources/android/icon/xxxhdpi.png" />
        <icon src="resources/android/icon/icon.png" />
        <splash density="land-hdpi" src="resources/android/screens/screen-hdpi-landscape.png" />
        <splash density="land-ldpi" src="resources/android/screens/screen-ldpi-landscape.png" />
        <splash density="land-mdpi" src="resources/android/screens/screen-mdpi-landscape.png" />
        <splash density="land-xhdpi" src="resources/android/screens/screen-xhdpi-landscape.png" />
        <splash density="hdpi" src="resources/android/screens/screen-hdpi-portrait.png" />
        <splash density="ldpi" src="resources/android/screens/screen-ldpi-portrait.png" />
        <splash density="mdpi" src="resources/android/screens/screen-mdpi-portrait.png" />
        <splash density="xhdpi" src="resources/android/screens/screen-xhdpi-portrait.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="ScrollEnabled" value="true" />
    </platform>
    <allow-navigation href="about:*" />
</widget>
